{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "influxdata/influxdb-client-php": "^2.3",
        "laravel/framework": "^8.12",
        "laravel/helpers": "^v1.4.1",
        "laravel/telescope": "^4.6",
        "laravel/ui": "^3.0",
        "league/flysystem-cached-adapter": "~1.0",
        "league/flysystem-sftp": "~1.0",
        "mattkingshott/axiom": "^3.2.1",
        "pusher/pusher-php-server": "^7.0",
        "rmccue/requests": "^v1.7.0",
      "ext-intl": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.8",
        "codeception/c3": "^2.6.2",
        "codeception/codeception": "*",
        "codeception/module-asserts": "^1.3.1",
        "codeception/module-laravel": "*",
        "codeception/module-phpbrowser": "^1.0.2",
        "codeception/notifier": "^0.1.0",
        "facade/ignition": "^2.5",
        "hoa/console": "*",
        "laracasts/cypress": "^1.4",
        "laravel/tinker": "^v2.6.1",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.0",
        "phpmd/phpmd": "^2.9.1",
        "phpstan/phpstan": "^0.12.99",
        "phpunit/phpunit": "^9.3.3",
        "scheldejonas/fzaninotto-faker": "^1.9",
        "squizlabs/php_codesniffer": "^3.6.0",
        "stechstudio/laravel-php-cs-fixer": "^v2.2.0",
        "vimeo/psalm": "^4.10"
    },
    "autoload": {
        "psr-4": {
            "Bank\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/_support"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "hooks": {
            "config": {
                "stop-on-failure": ["post-merge"]
            },
            "post-merge": [
                "\necho ========= Composer install =========\n",
                "composer install",
                "\necho ========= NPM install =========\n",
                "npm install",
                "\necho ========= NPM install =========\n",
                "npm install",
                "\necho ========= Database Migration =========\n",
                "php artisan migrate"
            ]
        },
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "codeception/c3": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
