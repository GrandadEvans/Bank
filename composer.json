{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-intl": "*",
        "ext-pdo": "*",
        "guzzlehttp/guzzle": "7.4.*",
        "influxdata/influxdb-client-php": "2.6.*",
        "laravel/framework": "8.83.*",
        "laravel/helpers": "1.5.*",
        "laravel/telescope": "4.8.*",
        "laravel/ui": "3.4.*",
        "league/flysystem-cached-adapter": "1.1.*",
        "league/flysystem-sftp": "1.*",
        "mattkingshott/axiom": "3.2.*",
        "pusher/pusher-php-server": "7.0.*",
        "rmccue/requests": "v2.0.*"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "2.8.*",
        "codeception/c3": "2.6.*",
        "codeception/codeception": "4.1.*",
        "codeception/module-asserts": "2.*",
        "codeception/module-laravel": "2.2.*",
        "codeception/module-phpbrowser": "2.0.*",
        "codeception/notifier": "0.1.*",
        "facade/ignition": "2.17.*",
        "hoa/console": "3.17.*",
        "laracasts/cypress": "2.2.*",
        "laravel/tinker": "v2.7.*",
        "mockery/mockery": "1.5.*",
        "nunomaduro/collision": "5.*",
        "phpmd/phpmd": "2.11.*",
        "phpunit/phpunit": "9.5.*",
        "scheldejonas/fzaninotto-faker": "1.9.*",
        "squizlabs/php_codesniffer": "3.6.*",
        "stechstudio/laravel-php-cs-fixer": "v3.1.*",
        "vimeo/psalm": "4.22.*"
    },
    "autoload": {
        "psr-4": {
            "Bank\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/_support"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "hooks": {
            "config": {
                "stop-on-failure": ["post-merge"]
            },
            "post-merge": [
                "\necho ========= Composer install =========\n",
                "composer install",
                "\necho ========= NPM install =========\n",
                "npm install",
                "\necho ========= NPM install =========\n",
                "npm install",
                "\necho ========= Database Migration =========\n",
                "php artisan migrate"
            ]
        },
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "codeception/c3": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
